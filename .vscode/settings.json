{
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 8,
    "prettier.tabWidth": 4,
    "files.exclude": {
        "**/.cache": true,
        "**/node_modules": true,
        "dist": true,
        "**/.DS_Store": true,
        "**/.env": true,
        "src/json": true,
        "src/csv": true,
        "data": true
    },
    "workbench.colorCustomizations": {
        "activityBar.background": "#322E0F",
        "titleBar.activeBackground": "#474015",
        "titleBar.activeForeground": "#FCFBF3"
    }
}

{
    "files.exclude": {
        "**/.cache": true,
        "**/node_modules": true,
        "dist": true,
        "**/.DS_Store": true,
        "**/.env": true,
        "src/json": true,
        "src/csv": true,
        "data": true
    },
    "workbench.colorCustomizations": {
        "activityBar.background": "#322E0F",
        "titleBar.activeBackground": "#474015",
        "titleBar.activeForeground": "#FCFBF3",
        "tab.activeBorder": "#f4f5f7",
        "tab.activeBorderTop": "#f4f5f7"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "[r]": {
        "editor.defaultFormatter": "Ikuyadeu.r"
    },
    "cSpell.words": [
        "displ"
    ],
    "r.plot.useHttpgd": true,
}
